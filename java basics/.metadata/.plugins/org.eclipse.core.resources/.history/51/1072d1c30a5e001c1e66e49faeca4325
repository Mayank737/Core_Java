package Oopsjava;

public class testaccount {
	public  double fund(double a, double b)
	{
		return(a-b);
	}
	public double withdrwal(double a, double b) {
		return(a-b);
	}
	public static void main(String[] args) {

		Account ab= new Account();
		ab.deposit(115.5, 78.5);

		Account b = new Account();
		b.setName("12345678990");
		b.setType("Current account");
		b.setBalance(100000.456);

		System.out.println(b.getName());
		System.out.println(b.getType());
		System.out.println(b.getBalance());

	}
}
