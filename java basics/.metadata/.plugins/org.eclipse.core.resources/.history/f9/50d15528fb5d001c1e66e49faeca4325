import java.util.*;
public class Person
{
    private String name;
    
	private Date dob;
	
	private String Address;
	
	String getName() 
	{
		return name;
	}
	 public void setName(String name)
	{
		this.name = name;
	}
	/*{
		SimpleDateFormat d = new SimpleDateFormat("dd/mm/yyyy");
		String dt = d.format(d);
		System.out.println(dt);
	}*/
	public Date getDob()
	{
		return dob;
	}
	public void setDob(Date dob)
	{
		this.dob = dob;
	}
	String getAddress()
	{
		return Address;
	}
	void setAddress(String address)
	{
		Address = address;
	}
	
}

	

