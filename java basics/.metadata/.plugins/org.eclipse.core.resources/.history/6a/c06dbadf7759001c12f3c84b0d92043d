package javaloops;

public class paldrornot {
	public static void main(String[] args) {
		
	
	int num=57,rem,temp,rev=0;
	temp=num;
	while(num>0)
	{
		rem=num%10;
		rev=rev*10+rem;
		rev=num/10;
	}
	if(temp==rev)
	System.out.println("number is palindrome"+num);
	else
	System.out.println("number is not palimdrome"+num);
}

}
