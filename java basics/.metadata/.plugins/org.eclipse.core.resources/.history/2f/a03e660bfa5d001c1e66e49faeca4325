package Oopsjava;

public class Account {
	private String name = null;
	private double balance = 0;
	private String type = null;
	String getName() {
		return name;
	}
	void setName(String name) {
		this.name = name;
	}
	double getBalance() {
		return balance;
	}
	void setBalance(double balance) {
		this.balance = balance;
	}
	String getType() {
		return type;
	}
	void setType(String type) {
		this.type = type;
	}
	
	
	

}
